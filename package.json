    "subfont": "^4.0.3"
  },
  "devDependencies": {
    "@netlify/build": "^0.1.7",
    "coveralls": "^3.0.2",
    "cpy-cli": "^2.0.0",
    "eslint": "^6.0.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-config-standard": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-mocha": "^6.2.0",
