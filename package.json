    "rimraf": "^3.0.0",
    "unexpected": "^11.0.0-1"
  },
  "main": "lib/index.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "cpy site dist",
    "lint": "eslint . && prettier --check '**/*.js'",
